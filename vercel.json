{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myblog_project.settings",
    "DEBUG": "False",
    "VERCEL_ENV": "production",
    "PYTHONPATH": "/var/task:/var/task/backend:/var/task/backend/Bac:/var/task/backend/Bac/myblog_project"
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "myblog_project.settings",
      "VERCEL_ENV": "production"
    }
  },
  "functions": {
    "wsgi.py": {
      "runtime": "python3.9"
    }
  }
}
